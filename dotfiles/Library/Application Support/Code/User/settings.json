{
  "cSpell.userWords": [
    "Hotp",
    "Inno",
    "Insertable",
    "Pylance",
    "Queryable",
    "actix",
    "byteorder",
    "cacher",
    "chrono",
    "clippy",
    "deps",
    "dotenv",
    "elly",
    "firstname",
    "gorm",
    "hasher",
    "hmac",
    "hmacsha",
    "hoge",
    "ipynb",
    "lastname",
    "nocapture",
    "noopener",
    "noreferrer",
    "openapi",
    "perlcritic",
    "perlcriticrc",
    "perltidy",
    "perltidyrc",
    "pypackages",
    "pyproject",
    "pytest",
    "setenv",
    "structopt",
    "stylelint",
    "termui",
    "timestep",
    "tomlkit",
    "toolchain",
    "totp",
    "ulwlu",
    "usecase",
    "varkey",
    "wasm"
  ],
  "editor.acceptSuggestionOnEnter": "on",
  "editor.codeActionsOnSave": {
    "source.organizeImports": true,
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "editor.cursorBlinking": "solid",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.fontFamily": "HackNerdFontComplete-Italic",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.insertSpaces": true,
  "editor.linkedEditing": true,
  "editor.matchBrackets": "never",
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {
    "other": true,
    "strings": true,
    "comments": true
  },
  "editor.smoothScrolling": true,
  "editor.snippetSuggestions": "top",
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.gotoLocation.multipleDeclarations": "gotoAndPeek",
  "editor.gotoLocation.multipleDefinitions": "gotoAndPeek",
  "editor.gotoLocation.multipleImplementations": "gotoAndPeek",
  "editor.gotoLocation.multipleReferences": "gotoAndPeek",
  "editor.gotoLocation.multipleTypeDefinitions": "gotoAndPeek",
  "emmet.includeLanguages": {
    "php": "html",
    "vue-html": "html",
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },
  "eslint.probe": [
    "vue",
    "html",
    "markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.compactFolders": false,
  "files.autoGuessEncoding": true,
  "files.autoSave": "off",
  "files.insertFinalNewline": false,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "[go]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "launch": {
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Program",
        "program": "${file}"
      }
    ]
  },
  "[perl]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "sfodje.perltidy"
  },
  "perlcritic.executable": "~/perl5/bin/perlcritic",
  "perlcritic.additionalArguments": ["--profile", "~/.perlcriticrc"],
  "perltidy.executable": "~/perl5/bin/perltidy",
  "perltidy.profile": "~/.perltidyrc",
  "python.defaultInterpreterPath": "/usr/local/bin/python3",
  "python.languageServer": "Pylance",
  "python.autoComplete.extraPaths": ["__pypackages__/3.9/lib"],
  "python.analysis.extraPaths": ["__pypackages__/3.9/lib"],
  "redhat.telemetry.enabled": true,
  "rust-client.channel": "stable",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "HackNerdFontComplete-Italic",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.profiles.osx": {
    "zsh": {
      "path": "/usr/local/bin/zsh"
    }
  },
  "window.title": "${activeEditorLong}${separator}${rootName}",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.fontAliasing": "antialiased",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.editor": "json",
  "workbench.tree.indent": 16,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "workbench.colorTheme": "Ayu Dark",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": ["variable.parameter", "meta.parameter"],
        "settings": {
          "foreground": "#b3b1ad"
        }
      }
    ]
  }
}
